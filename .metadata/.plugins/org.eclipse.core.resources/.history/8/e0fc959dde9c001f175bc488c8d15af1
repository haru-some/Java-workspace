package kh.java.func;

public class OperatorTest {
	public void test1() {
		//산술연산자(+,-,*,/,%)
		//대입연산자(=)	a = b; -> b라는 값을 a라는 공간(변수)에 대입
		
		//복합대입 연산자 : 산술연산자와 대입연산자가 합쳐진 형태의 연산자
		//+=, -=, *=, /=, %=
		//산술연산을 한 결과를 바로 변수에 대입까지 하는 연산자
		int num = 10;
		System.out.println("num : "+num);
		num += 5;
		System.out.println("num : "+num);
	}
}
